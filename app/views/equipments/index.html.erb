 <% @equipment.each do |equipment| %>
  <div class="index_equipment_flex d-flex justify-content-around">
    <h1><%= equipment.name %></h1>
    <p><%= equipment.identifiant %></p>
    <p><%= equipment.location %></p>
    <p><%= equipment.detail %></p>
    <p><%= equipment.commissioning_date %></p>
    <p><%= equipment.validity_qualification %></p>
    <p><%= equipment.status %></p>
  </div>
<% end %>
